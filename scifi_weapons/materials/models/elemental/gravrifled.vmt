"VertexLitGeneric"
{
	$baseTexture 				"dev/hide"
	// $bumpmap 					"shadertest/noise_normal"

	// $envmap 					"env_cubemap"
	// $normalmapalphaenvmapmask 	1
	// $envmaptint 				"[ 0.2 0.2 0.3 ]"
	// $envmapconstrast 			1
	// $surfaceprop 				"default_silent"

	// $model 						1
	$nodecal 					1
	$alphatest 					1
	// $translucent 				1
	$additive 					1

	$selfillumt 				1
	$selfillumtint 				"[ 1 1 1 ]"
	
	$emissiveBlendEnabled 		1
	$emissiveBlendTexture		"models/elemental/gravrifled_noise" // "models/mana/shield_noise"
	$emissiveBlendBaseTexture	"vgui/white"
	$emissiveBlendFlowTexture	"models/elemental/gravrifled_flow" // "models/mana/shield_flow"
	$emissiveBlendTint 			"[ 0.4 0.6 6 ]"
	$emissiveBlendStrength 		1
	$emissiveBlendScrollVector 	"[ -0.5 1 ]"
	
	$SphereMaskOrigin 			"[ 0 0 0 ]"
	$SphereMaskRadius 			0

	$FleshInteriorEnabled      	1
	$FleshInteriorTexture      	"vgui/black"
	$FleshNormalTexture			"shadertest/noise_normal" 
	$FleshBorderTexture1D      	"models/weapons/misc/energy_surf_tintmap_celestial"
	$FleshInteriorNoiseTexture 	"engine/noise-blur-256x256"
	$FleshSubsurfaceTexture	 	"models/black"
	$FleshSubsurfaceTint 		"[ 1 1 1 ]"
	$FleshCubeTexture          	"models/elemental/vapored_tintmap"
	$FleshBorderNoiseScale     	0.5
	$FleshBorderWidth			4
	$FleshBorderSoftness		0.1
	$FleshBorderTint			"[ 2 0.6 4 ]"
	$FleshGlossBrightness		4
	$FleshDebugForceFleshOn	 	1
	$FleshScrollSpeed			1.0
	
	$FleshEffectCenterRadius	"[ 0 0 0 1 ]"
//	$FleshEffectCenterRadius1	656
//	$FleshEffectCenterRadius2	-480
//	$FleshEffectCenterRadius3	-32
//	$FleshEffectCenterRadius4	256
	$FleshGlobalOpacity 		1
	
	$energycolor 				"[ 1 0.4 6 ]"
	
	$time 1
	
	// The flesh effect only really works when combined with the debug option above.
	// GMOD doesn't have the point_flesh_effect_target entity by default and can't really feed data into the material proxy either.

	"Proxies" 
	{
		"FleshInterior"
		{
		}
		
		"Equals"
		{
			"srcVar1" 		$energycolor
			"resultVar" 	$selfillumtint
		}
		
		"Multiply"
		{
			"srcVar1" 		$energycolor
			"srcVar2"		$emissiveBlendStrength
			"resultVar" 	$emissiveBlendTint
		}
		
		"Equals"
		{
			"srcVar1" 		$SphereMaskOrigin
			"resultVar" 	"$FleshEffectCenterRadius[1]"
		}
		
		"Equals"
		{
			"srcVar1" 		$SphereMaskOrigin
			"resultVar" 	"$FleshEffectCenterRadius[2]"
		}
		
		"Equals"
		{
			"srcVar1" 		$SphereMaskOrigin
			"resultVar" 	"$FleshEffectCenterRadius[3]"
		}
		
		"Equals"
		{
			"srcVar1" 		$SphereMaskRadius
			"resultVar" 	"$FleshEffectCenterRadius[4]"
		}
	}
}